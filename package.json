{
  "name": "QAMdemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rm -R -f reports && mkdir reports",
    "clean:serenity": "rimraf target",
    "pretest": "npm run clean:reports || npm run clean:serenity",
    "cypress:open": "cypress open --env configFile=${environment}",
    "cypress:run:spec": "cypress run -b ${browser} --headless --reporter-options --env configFile=${environment} -s ${spec} -- --record ",
    "cypress:run": "cypress run --headless --env configFile=${environment}",
    "combine-reports": "mochawesome-merge './reports/wesome/*.json' -o './reports/wesome/reports.json'",
    "generate-report": "marge reports/wesome/reports.json -f report -o reports/html",
    "posttest": "npm run combine-reports && npm run generate-report &&  npm run test:serenity:report",
    "test": "npm run clean:reports && npm run cypress:run:spec || npm run posttest",
    "download-extension": "ced gleekbfjekiniecknbkamfmkohkpodhe extensions/ignore-x-frame-headers",
    "test:update-serenity": "serenity-bdd update --ignoreSSL",
    "test:acceptance": "cucumber-js",
    "test:serenity:report": "serenity-bdd run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.1",
    "@serenity-js/console-reporter": "^2.32.2",
    "@serenity-js/core": "^2.32.2",
    "@serenity-js/cucumber": "^2.32.2",
    "@serenity-js/rest": "^2.32.2",
    "@serenity-js/serenity-bdd": "^2.32.2",
    "@types/assert": "^1.5.6",
    "chrome-ext-downloader": "^1.0.4",
    "cypress": "5.4.0",
    "cypress-cucumber-preprocessor": "^2.5.4",
    "cypress-file-upload": "^4.0.7",
    "cypress-multi-reporters": "^1.2.4",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-serenity-reporter": "^1.0.1",
    "cypress-xpath": "^1.6.1",
    "mocha": "^7.1.2",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^5.1.0",
    "npm-failsafe": "^0.4.3",
    "path": "^0.12.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
