{
  "baseUrl": "https://rb-pasivo-dev-front.avaldigitallabs.com",
  "reporter": "cypress-multi-reporters",
  "reporterEnabled": "mochawesome, mocha-junit-reporter,cypress-serenity-reporter",
  "mochaJunitReporterReporterOptions": {
    "mochaFile": "target/junit/test-results.[hash].xml",
    "testsuitesTitle": false
  },
  "mochawesomeReporterOptions": {
    "reportDir": "target/wesome",
    "reportFilename": "mochawesomeReport_[hash].json",
    "overwrite": false,
    "html": false,
    "json": true
  },
  "cypressSerenityReporterOptions": {
    "reportDir": "target/serenity",
    "html": true
  },
  "projectId": "fqvc7y",
  "env": {
    "CAPCTHA_LOW_SCORE": "lowscore",
    "CAPCTHA_TIME_OUT": "timeout",
    "CAPCTHA_OK": "ok",
    "CAPCTHA_500": "500"
  },
  "video": true,
  "failOnStatusCode": false,
  "supportFile": "./cypress/support/index.js",
  "screenshotsFolder": "target/site/cypress/screenshots",
  "videosFolder": "target/site/cypress/videos",
  "chromeWebSecurity": false,
  "modifyObstructiveCode": false,
  "viewportWidth": 1100,
  "viewportHeight": 1000,
  "experimentalNetworkStubbing": false
}

