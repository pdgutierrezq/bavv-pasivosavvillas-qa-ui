{
  "reporter": "cypress-multi-reporters",
  "reporterEnabled": "mochawesome, mocha-junit-reporter,cypress-dashboard,cypress-serenity-reporter",
  "mochaJunitReporterReporterOptions": {
    "mochaFile": "reports/junit/test-results.[hash].xml",
    "testsuitesTitle": false
  },
  "mochawesomeReporterOptions": {
    "reportDir": "reports/wesome",
    "reportFilename": "mochawesomeReport_[hash].json",
    "overwrite": false,
    "html": false,
    "json": true
  },
  "cypressDashboardReporterOptions": {
    "reportDir": "reports/dashboard",
    "isLocalDeployment": true
  },
  "cypressSerenityReporterOptions": {
    "reportDir": "reports/serenity",
    "html": true
  },
  "projectId": "fqvc7y",
  "env": {
    "CAPCTHA_LOW_SCORE": "lowscore",
    "CAPCTHA_TIME_OUT": "timeout",
    "CAPCTHA_OK": "ok",
    "CAPCTHA_500": "500"
  },
  "video": true,
  "failOnStatusCode": false,
  "supportFile": "./cypress/support/index.js",
  "screenshotsFolder": "./reports/screenshots",
  "videosFolder": "./reports/videos",
  "chromeWebSecurity": false,
  "modifyObstructiveCode": false,
  "viewportWidth": 1100,
  "viewportHeight": 1000
}

